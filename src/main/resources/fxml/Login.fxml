<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="171.0" prefWidth="326.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
   <children>
      <TextField fx:id="textLogin" layoutX="10.0" layoutY="16.0" prefHeight="26.0" prefWidth="301.0" promptText="Digite Login" />
      <TextField fx:id="textPassword" layoutX="10.0" layoutY="52.0" prefHeight="26.0" prefWidth="301.0" promptText="Digite Senha" />
      <Button fx:id="btnLogin" layoutX="12.0" layoutY="94.0" mnemonicParsing="false" onAction="#login" prefHeight="20.0" prefWidth="302.0" text="Login" />
      <Button fx:id="btnCancel" layoutX="12.0" layoutY="128.0" mnemonicParsing="false" onAction="#cancel" prefHeight="20.0" prefWidth="302.0" text="Cancelar" />
   </children>
</AnchorPane>
